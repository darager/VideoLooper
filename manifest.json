{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "commands": {
    "play-pause": {
      "description": "Resume/Pause video"
    },
    "change-speed": {
      "suggested_key": "Alt+I",
      "description": "Change speed of video"
    },
    "move-video-forward": {
      "suggested_key": "Alt+L",
      "description": "Forward video 5 seconds"
    },
    "move-video-back": {
      "suggested_key": "Alt+J",
      "description": "Reverse video 5 seconds"
    },
    "toogle-loop": {
      "suggested_key": "Alt+U",
      "description": "Toogle video loop"
    }
  }
}
