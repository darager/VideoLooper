{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "activeTab", "tabs", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "commands": {
    "play-pause": {
      "suggested_key": "Alt+K",
      "description": "Resume/Pause Video"
    },
    "move-video-forward": {
      "description": "Forward video a few seconds"
    },
    "move-video-back": {
      "suggested_key": "Alt+J",
      "description": "Reverse video a few seconds"
    },
    "toggle-speed": {
      "suggested_key": "Alt+I",
      "description": "Change video speed"
    },
    "increase-speed": {
      "description": "Increase video speed"
    },
    "decrease-speed": {
      "description": "Decrease video speed"
    },
    "reset-speed": {
      "description": "Toggle between 1x and prefered video speed"
    },
    "toggle-loop": {
      "suggested_key": "Alt+U",
      "description": "Control video looping"
    },
    "set-loop-start": {
      "description": "Set start of video loop"
    },
    "set-loop-end": {
      "description": "Set end of video loop"
    },
    "remove-loop": {
      "description": "Remove video loop"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["videoController.js"]
    }
  ]
}
